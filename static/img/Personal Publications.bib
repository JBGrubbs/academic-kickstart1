	note = {type: article},
	keywords = {pornography, hypersexuality, sex addiction, anxiety, Anxiety Disorders, Clinical Psychology, compulsive sexual behaviour, Social and Behavioral Sciences, and Conduct Disorders, Disruptive, Impulse-control, Substance Abuse and Addiction},
	file = {Full Text PDF:/Volumes/GoogleDrive/My Drive/Manuscripts/Zotero/storage/BE5KBDZ9/Grant et al. - 2021 - Anxiety and Compulsive Sexual Behaviour Disorder .pdf:application/pdf},
