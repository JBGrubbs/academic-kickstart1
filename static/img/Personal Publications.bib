	title = {Pornography {Use} and {Psychological} {Science}: {A} {Call} for {Consideration}},
	copyright = {All rights reserved},
	issn = {0963-7214, 1467-8721},
	shorttitle = {Pornography {Use} and {Psychological} {Science}},
