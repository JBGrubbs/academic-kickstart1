	keywords = {INTERNET pornography, SEX addiction, PORNOGRAPHY -- Religious aspects, RESEARCH, DISTRESS (Psychology), FACTOR analysis, GUILT (Psychology), RELIABILITY (Personality trait), SEX customs},
	pages = {106--126},
